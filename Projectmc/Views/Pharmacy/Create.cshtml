@model Projectmc.Models.pharmacy

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>
@using (Html.BeginForm())
{
    <div>
        <label>Pharmacy Name</label>
        <div>
            @Html.EditorFor(m => m.PharmacyName)
        </div>
    </div>
    <div>
        <label>Pharmacy Address</label>
        <div>
            @Html.EditorFor(m => m.PharmacyAddress)
        </div>
    </div>
    <div>
        <label>Manager</label>
        <div>
            @Html.EditorFor(m => m.Manager)
        </div>
    </div>
    <div>
        <label>Drug Name</label>
        <div>
            @Html.DropDownList("DrugId", null, new { @class = "form-control" })
        </div>
    </div>
    <div>
        <input type="submit" value="Save" />
    </div>
}

